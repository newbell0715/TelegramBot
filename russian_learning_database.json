  "conversations": [
    {"id": 1, "russian": "Как дела?", "korean": "어떻게 지내?", "pronunciation": "깍 델라?", "level": "beginner", "category": "greetings"},
    {"id": 2, "russian": "Что нового?", "korean": "뭐 새로운 거 있어?", "pronunciation": "쉬토 노바바?", "level": "beginner", "category": "greetings"},
    {"id": 3, "russian": "Увидимся завтра!", "korean": "내일 봐요!", "pronunciation": "우비짐시야 잡트라!", "level": "beginner", "category": "farewell"},
    {"id": 4, "russian": "Доброе утро!", "korean": "좋은 아침!", "pronunciation": "도브로예 우트라!", "level": "beginner", "category": "greetings"},
    {"id": 5, "russian": "Добрый день!", "korean": "안녕하세요!", "pronunciation": "도브리 젠!", "level": "beginner", "category": "greetings"},
    {"id": 6, "russian": "Добрый вечер!", "korean": "좋은 저녁!", "pronunciation": "도브리 볘체르!", "level": "beginner", "category": "greetings"},
    {"id": 7, "russian": "Спокойной ночи!", "korean": "안녕히 주무세요!", "pronunciation": "스파코이나이 노치!", "level": "beginner", "category": "farewell"},
    {"id": 8, "russian": "Меня зовут...", "korean": "제 이름은...입니다", "pronunciation": "미냐 자부트...", "level": "beginner", "category": "introduction"},
    {"id": 9, "russian": "Как вас зовут?", "korean": "성함이 어떻게 되세요?", "pronunciation": "깍 바스 자부트?", "level": "beginner", "category": "introduction"},
    {"id": 10, "russian": "Приятно познакомиться!", "korean": "만나서 반가워요!", "pronunciation": "프리야트나 파즈나코미치샤!", "level": "beginner", "category": "introduction"},
    {"id": 11, "russian": "Сколько вам лет?", "korean": "몇 살이세요?", "pronunciation": "스콜카 밤 렛?", "level": "beginner", "category": "introduction"},
    {"id": 12, "russian": "Мне 25 лет.", "korean": "저는 25살입니다.", "pronunciation": "므네 드바드짜치 파치 렛.", "level": "beginner", "category": "introduction"},
    {"id": 13, "russian": "Откуда вы?", "korean": "어디에서 오셨어요?", "pronunciation": "아트쿠다 비?", "level": "beginner", "category": "introduction"},
    {"id": 14, "russian": "Я из Кореи.", "korean": "저는 한국에서 왔습니다.", "pronunciation": "야 이즈 카레이.", "level": "beginner", "category": "introduction"},
    {"id": 15, "russian": "Сколько это стоит?", "korean": "이것은 얼마입니까?", "pronunciation": "스콜카 에타 스토잇?", "level": "beginner", "category": "shopping"},
    {"id": 16, "russian": "Это очень дорого.", "korean": "이것은 매우 비싸요.", "pronunciation": "에타 오첸 도로가.", "level": "beginner", "category": "shopping"},
    {"id": 17, "russian": "Можно посмотреть?", "korean": "봐도 될까요?", "pronunciation": "모즈나 파스마트례치?", "level": "beginner", "category": "shopping"},
    {"id": 18, "russian": "Я буду это.", "korean": "이것으로 하겠습니다.", "pronunciation": "야 부두 에타.", "level": "beginner", "category": "shopping"},
    {"id": 19, "russian": "Где касса?", "korean": "계산대가 어디에 있나요?", "pronunciation": "그데 까싸?", "level": "beginner", "category": "shopping"},
    {"id": 20, "russian": "Можно меню?", "korean": "메뉴 주세요.", "pronunciation": "모즈나 미뉴?", "level": "beginner", "category": "restaurant"},
    {"id": 21, "russian": "Что вы рекомендуете?", "korean": "뭘 추천하세요?", "pronunciation": "쉬토 비 리카멘두예체?", "level": "beginner", "category": "restaurant"},
    {"id": 22, "russian": "Я буду борщ.", "korean": "보르시 주세요.", "pronunciation": "야 부두 보르시.", "level": "beginner", "category": "restaurant"},
    {"id": 23, "russian": "Счёт, пожалуйста.", "korean": "계산서 주세요.", "pronunciation": "쇼트, 파좔류스타.", "level": "beginner", "category": "restaurant"},
